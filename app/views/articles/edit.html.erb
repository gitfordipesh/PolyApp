<h1>Edit Article</h1>

<%= form_with(model: @article, url: article_path) do |form| %>
  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>
  <p>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
  </p>
  <p>
    <%= form.submit class:"btn btn-secondary"%>
  </p>
<% end %>