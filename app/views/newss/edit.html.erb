<h1>Edit News</h1>

<%= form_with(model: @news, url: newss_path) do |form| %>
  <div class="form-group">
    <%= form.label :title %>
    <%= form.text_field :title, class:"form-control"%>
  </div>

  <div class="form-group">
    <%= form.label :is_live %>
    <%= form.check_box :is_live %>
  </div>

  <div class="form-group">
    <%= form.submit class:"btn btn-secondary"%>
  </div>
<% end %>